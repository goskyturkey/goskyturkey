# GoSky Turkey Backend Environment Variables
# Copy this file to .env and update values

# Server
PORT=3000
NODE_ENV=production

# Database
MONGODB_URI=mongodb://mongodb:27017/goskyturkey

# JWT
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRES_IN=7d

# CORS
ALLOWED_ORIGINS=https://goskyturkey.com,http://localhost:3001

# iyzico Payment (Sandbox)
IYZICO_API_KEY=sandbox-api-key
IYZICO_SECRET_KEY=sandbox-secret-key
IYZICO_URI=https://sandbox-api.iyzipay.com
